{
    "extends": ["standard", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "plugins": ["react"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "camelcase": ["error", {"properties": "never"}]
    },
    "globals": { "fetch": false }
}
